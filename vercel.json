{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "https://api.mio-hub.me/api/auth/:path*"
    },
    {
      "source": "/api/mihub/get-messages",
      "destination": "/api/mihub/get-messages"
    },
    {
      "source": "/api/mihub/:path*",
      "destination": "https://api.mio-hub.me/api/mihub/:path*"
    },
    {
      "source": "/api/guardian/:path*",
      "destination": "https://api.mio-hub.me/api/guardian/:path*"
    },
    {
      "source": "/api/mio/:path*",
      "destination": "https://api.mio-hub.me/api/mio/:path*"
    },
    {
      "source": "/api/abacus/:path*",
      "destination": "https://api.mio-hub.me/api/abacus/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
